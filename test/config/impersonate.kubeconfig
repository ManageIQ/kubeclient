apiVersion: v1
clusters:
- cluster:
    server: https://localhost:8443
    insecure-skip-tls-verify: true
  name: localhost:8443
contexts:
- context:
    cluster: localhost:8443
    namespace: default
    user: impersonate
  name: localhost/impersonate
current-context: localhost/impersonate
kind: Config
preferences: {}
users:
- name: impersonate
  user:
    as: foo
    as-groups: [bar, baz]
    as-user-extra:
      reason: [foo]
